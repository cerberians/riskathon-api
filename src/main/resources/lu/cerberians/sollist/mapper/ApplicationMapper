<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="lu.cerberians.sollist.mapper.ApplicationMapper">

    <select id="selectAll" resultType="lu.cerberians.sollist.entities.Application">
        MATCH (asset:Asset)-[:HAS_ASSET]-(app)
        RETURN asset.id,asset.name,app.id,app.name
        MATCH (n:Application) RETURN n.id as id, n.name as name
    </select>

</mapper>